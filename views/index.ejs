                                <% }else{%>
                                <span class="r_ ix_color2" title="<%=guzhiData[i]["Swing"] %> | <%=guzhiData[i]["SwingRange"] %>"><%=guzhiData[i]["Swing"] %> | <%=guzhiData[i]["SwingRange"] %></span>
                                <% } %>
                            </div>
                        <% } %>
                    </dd>
                </div>
