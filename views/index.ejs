                                <% }else{%>
                                <span class="r_ ix_color2" title="<%=shangpingData[i]["Swing"] %> | <%=shangpingData[i]["SwingRange"] %>"><%=shangpingData[i]["Swing"] %> | <%=shangpingData[i]["SwingRange"] %></span>
                                <% } %>
                            </div>
                        <% } %>
                    </dd>
                    <dd>
                         <%  for(var i=0; i<waihuiData.length; i++) { %>
                           <div class="ix_sp <%=waihuiData[i]["Code"] %>" data-code="<%=waihuiData[i]["Code"] %>">
