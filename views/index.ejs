                                <% }else{%>
                                <span class="r_ ix_color2"  title="<%=waihuiData[i]["Swing"] %> | <%=waihuiData[i]["SwingRange"] %>"><%=waihuiData[i]["Swing"] %> | <%=waihuiData[i]["SwingRange"] %></span>
                                <% } %>
                            </div>
                        <% } %>
                    </dd>
                    <dd>
                        <%  for(var i=0; i<guzhiData.length; i++) { %>
                           <div class="ix_sp <%=guzhiData[i]["Code"] %>" data-code="<%=guzhiData[i]["Code"] %>">
                                <span class="ix_color" title="<%=guzhiData[i]["Name"] %>"><%=guzhiData[i]["Name"] %></span>
                                <span  class="ix_color1" title="<%=guzhiData[i]["Last"] %>"><%=guzhiData[i]["Last"] %></span>
                                <% if(guzhiData[i]["Swing"]*1<0){ %>
