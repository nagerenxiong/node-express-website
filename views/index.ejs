            </dt>
            <% var curDate = new Date();var currentDate = curDate.toLocaleDateString();for(var i=firstData["data"].length-1; i>=0; i--) { %>
             <% if(firstData["data"][i]["time"].substr(0,10)!=currentDate){%>
               <dd class="dateClass " ><p><% var dateLast=firstData["data"][i-1]["time"].substr(0,10);var arys1= new Array();arys1=dateLast.split('-'); var ssdate=new Date(arys1[0],parseInt(arys1[1]-1),arys1[2]); var dayIndex=ssdate.getDay();switch (dayIndex) {case 1: dayIndex = '星期一';break; case 2: dayIndex = '星期二';break; case 3: dayIndex = '星期三';break; case 4: dayIndex = '星期四';break; case 5: dayIndex = '星期五';break; case 6: dayIndex = '星期六';break; case 0: dayIndex = '星期天';break; }  %><%=dateLast%> <%=dayIndex%></p></dd>
             <% currentDate=firstData["data"][i]["time"].substr(0,10) } %>
             <% if(firstData["data"][i]["type"]==2) { %>
                <dd data-idg="<%=firstData["data"][i]["id"]%>" data-time="<%=firstData["data"][i]["time"].substr(0,10) %>" data-id="<%=firstData["kxLastid"]%>" class="classTypeTwo">
                <span ><%=firstData["data"][i]["time"].substr(10,6) %></span>
                 <div class="ix_txt">
                       <% if(firstData["data"][i]["state"]=="美国"){ %>
                        <img src="images/cr_cy01.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="中国"){ %>
                        <img src="images/cr_cy02.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="欧元区"){ %>
                        <img src="images/cr_cy03.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="日本"){ %>
                        <img src="images/cr_cy04.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="澳大利亚"){ %>
                        <img src="images/cr_cy05.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="加拿大"){ %>
                        <img src="images/cr_cy07.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="瑞士"){ %>
                        <img src="images/cr_cy08.jpg" width="42" height="42">
                        <% } else if(firstData["data"][i]["state"]=="英国"){ %>
                        <img src="images/cr_cy06.jpg" width="42" height="42">
                         <% } else if(firstData["data"][i]["state"]=="台湾"){ %>
                        <img src="images/cr_cy09.jpg" width="42" height="42">
                        <% } else if(firstData["data"][i]["state"]=="韩国"){ %>
                        <img src="images/hg2.png" width="42" height="42">
                        <% } else if(firstData["data"][i]["state"]=="德国"){ %>
                        <img src="images/dg.png" width="42" height="42">
                        <%} else if(firstData["data"][i]["state"]=="法国"){ %>
                        <img src="images/fg.jpg" width="42" height="42">
                        <% } else if(firstData["data"][i]["state"]=="西班牙"){ %>
                        <img src="images/xby.png" width="42" height="42">
                       <% }else if(firstData["data"][i]["state"]=="新西兰"){ %>
                        <img src="images/xxl.jpg" width="42" height="42">
                        <% }else if(firstData["data"][i]["state"]=="意大利"){ %>
                        <img src="images/ydl2.png" width="42" height="42">
                        <% }else if(firstData["data"][i]["state"]=="斯洛伐克"){ %>
                        <img src="images/ydl2.png" width="42" height="42">
                        <% }else if(firstData["data"][i]["state"]=="希腊"){ %>
                        <img src="images/xl.png" width="42" height="42">
                        <% } %>
                        <div class="ix_desc">
                            <h2><%=firstData["data"][i]["state"] %> <%=firstData["data"][i]["title"] %> </h2>
                            <span>前值：<%=firstData["data"][i]["before"] %>  预期：<%=firstData["data"][i]["forecast"] %></span>
                        </div>
                        <div class="ix_val" data-imp='<%=firstData["data"][i]["importance"]%>'>
                           <% if(firstData["data"][i]["importance"]=="高"){ %>
                                <img src="images/ix_degree2.jpg" width="30" height="24">
                           <%  } else if(firstData["data"][i]["importance"]=="中"){ %>
                             <img src="images/ix_degree.jpg" width="30" height="24">
                            <% }else{ %>
                                 <img src="images/ix_degree3.jpg" width="30" height="24">
                            <% } %>
                            <span>实际值：<%=firstData["data"][i]["reality"] %></span>
                        </div>
                        <div class="ix_effect" >
                        <% if(firstData["data"][i]["effect"]=="||") { %>
                            <span  style="font-size: 12px;color:#FD9525">影响较小</span>
                            <%  } %>
                            <% var index=firstData["data"][i]["effect"].indexOf("|");var ld=firstData["data"][i]["effect"].substring(0,index).replace(/\|/g,"");var lk=firstData["data"][i]["effect"].substring(index).replace(/\|/g,""); %>
                                <% if(ld!=""&&ld.length!=0){ %>
                                   <% var ldArray=ld.split(" ");var lkArray=lk.split(" ") %>
                                    <span  style="font-size: 12px;">
                                    <em style="color:red"> 利多:
                                    <% for(var k=0;k<ldArray.length;k++) {%>
                                      <b data-type="<%=ldArray[k] %>" style="color:red"><%=ldArray[k] %></b>
                                    <% } %>
                                    </em>
                                        <% if(lk!=""&&lk.length!=0) {%>
                                        <em style="color:green">利空:
                                        <% for(var k=0;k<lkArray.length;k++) {%>
                                        <b data-type="<%=lkArray[k] %>" style="color:green"><%=lkArray[k] %></b>
                                        <%} %>
                                        </em>
                                        <% } %>
                                    </span>
                                <%} else { %>
                                        <% var lkArray=lk.split(" ") %>
                                        <% if(lk!=""&&lk.length!=0) {%>
                                        <span  style="font-size: 12px;"><em style="color:green">利空:
                                        <% for(var k=0;k<lkArray.length;k++) {%>
                                           <% if(lkArray[k]!=""){ %>
                                          <b style="color:green" data-type="<%=lkArray[k] %>"><%=lkArray[k] %></b>
                                          <% } %>
                                        <% } %>
                                        </em></span>
                                        <% } %>
                        <% } %>
                            </span>
                        </div>
                    </div>
                </dd>
                 <% } else { %>
                   <% if(firstData["data"][i]["importance"]=="高") {%>
                   <dd data-idg="<%=firstData["data"][i]["id"]%>" class="ix_cn_dd" data-id="<%=firstData["kxLastid"]%>" data-time="<%=firstData["data"][i]["time"].substr(0,10) %>">
                   <% } else { %>
                   <dd data-idg="<%=firstData["data"][i]["id"]%>" data-id="<%=firstData["data"][i]["autoid"] %>" data-time="<%=firstData["data"][i]["time"].substr(0,10) %>">
                    <% } %>
                    <span><%=firstData["data"][i]["time"].substr(10,6) %></span>
                        <p>
                        <%-firstData["data"][i]["title"]%></p>
                </dd>
                   <% } %>
              <% } %>
        </dl>
        <button class="load_more" onclick="next_page()">点击加载更多... </button>
