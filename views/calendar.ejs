</div>
<div class="data">
	<div class="da_form">
    	<h1><span class="curDate"><%=currentDate%></span> 经济数据一览</h1>
        <table border="0"  cellpadding="0" cellspacing="0">
        	<tr>
            	<th>时间</th>
                <th>国家（地区）</th>
                <th class="da_data">指标名称</th>
            	<th>重要性</th>
                <th>前值</th>
                <th>预测值</th>
                <th>公布值</th>
                <th class="da_data">多空判断</th>
            </tr>
             <% for (var i = 0; i < data.length; i++) { %>
                <tr>
                <td><%=data[i]["PredictTime"]%></td>
                <td class="da_logo" data-c="<%=data[i]["State"]%>">
                    <% if(data[i]["State"]=="美国"){ %>
                    <img src="images/cr_cy01.jpg" width="21" height="14">
                    <%} else if(data[i]["State"]=="中国") { %>
                    <img src="images/cr_cy02.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="欧盟") { %>
                    <img src="images/cr_cy03.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="日本") { %>
                    <img src="images/cr_cy04.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="澳大利亚") { %>
                    <img src="images/cr_cy05.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="英国") { %>
                    <img src="images/cr_cy06.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="加拿大") { %>
                    <img src="images/cr_cy07.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="瑞士") { %>
                    <img src="images/cr_cy08.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="台湾") { %>
                    <img src="images/cr_cy09.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="香港") { %>
                    <img src="images/cr_cy09.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="新西兰") { %>
                    <img src="images/cr_cy09.jpg" width="21" height="14">
                     <%} else if(data[i]["State"]=="西班牙") { %>
                    <img src="images/xby.png" width="21" height="14">
                     <%} else if(data[i]["State"]=="韩国") { %>
                    <img src="images/hg.png" width="21" height="14">
                     <%} else if(data[i]["State"]=="意大利") { %>
                    <img src="images/ydl.png" width="21" height="14">
                     <%} else if(data[i]["State"]=="法国") { %>
                    <img src="images/fg.png" width="21" height="14">
                     <%} else if(data[i]["State"]=="德国") { %>
                    <img src="images/dg.png" width="21" height="14">
                      <%} else if(data[i]["State"]=="希腊") { %>
                    <img src="images/xl.png" width="21" height="14">
                     <%} else if(data[i]["State"]=="欧元区") { %>
                    <img src="images/oyq.png" width="21" height="14">
                    <% } %>
                </td>
                <td>
                    <%  for(var j=i;j<data.length;j++){ %>
                      <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %>
                          <span title="<%=data[j]["NFI"]%>"><%=data[j]["State"]%><%=data[j]["NFI"]%></span>
                      <% } %>
                    <% } %>
                </td>
                <td class="da_import" >
                <%  for(var j=i;j<data.length;j++){ %>
                      <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %>
                    <span class="da_mid importSpan" data-i="<%=data[j]["Nature"]%>">
                    <% if(data[j]["Nature"]=="高"){ %>
                    <img src="images/ix_degree2.jpg" width="28" height="22">
                    <%} else if(data[j]["Nature"]=="中") { %>
                    <img src="images/ix_degree.jpg" width="28" height="22">
                     <%} else if(data[j]["Nature"]=="低") { %>
                      <img src="images/ix_degree3.jpg" width="28" height="22">
                     <% } %>
                    </span>
                   <% } %>
                    <% } %>
                </td>
                <td><%  for(var j=i;j<data.length;j++){ %>
                      <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %><span><%=data[j]["Before"]%></span><%}} %></td>
                <td><%  for(var j=i;j<data.length;j++){ %>
                  <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %><span><%=data[j]["Forecast"]%></span><%}}%></td>
                <td><%  for(var j=i;j<data.length;j++){ %>
                  <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %><span><%=data[j]["Reality"]%></span><%}}%></td>
                <td class="da_judge">
                <%  for(var j=i;j<data.length;j++){ %>
                      <% if(data[i]["PredictTime"]==data[j]["PredictTime"]&&data[i]["State"]==data[j]["State"]){ %>
                    <% if(data[j]["Effect"]=="||") { %>
                    <span  style="font-size: 12px;color:#FD9525">影响较小</span>
                    <%  } %>
                    <% var index=data[j]["Effect"].indexOf("|");var ld=data[j]["Effect"].substring(0,index).replace(/\|/g,"");var lk=data[j]["Effect"].substring(index).replace(/\|/g,""); %>
                        <% if(ld!=""&&ld.length!=0){ %>
                           <% var ldArray=ld.split(" ");var lkArray=lk.split(" ") %>
                            <span  style="font-size: 12px;">
                            <em style="color:red"> 利多:
                            <% for(var k=0;k<ldArray.length;k++) {%>
                              <b data-type="<%=ldArray[k] %>" style="color:red"><%=ldArray[k] %></b>
                            <% } %>
                            </em>
                                <% if(lk!=""&&lk.length!=0) {%>
                                <em style="color:green">利空:
                                <% for(var k=0;k<lkArray.length;k++) {%>
                                <b data-type="<%=lkArray[k] %>" style="color:green"><%=lkArray[k] %></b>
                                <%} %>
                                </em>
                                <% } %>
                            </span>
                        <%} else { %>
                                <% var lkArray=lk.split(" ") %>
                                <% if(lk!=""&&lk.length!=0) {%>
                                <span  style="font-size: 12px;"><em style="color:green">利空:
                                <% for(var k=0;k<lkArray.length;k++) {%>
                                   <% if(lkArray[k]!=""){ %>
                                  <b style="color:green" data-type="<%=lkArray[k] %>"><%=lkArray[k] %></b>
                                  <% } %>
                                <% } %>
                                </em></span>
                                 <% } else{ if(data[j]["Effect"]!="||"){ %>
                               <span></span>
                                <% } }%>
                        <% } %>
                    <% i=j }}%>
                </td>
            </tr>
           <% } %>
        </table>
    </div>
	<div class="da_form">
    	<h1><span class="curDate"><%=currentDate%></span> 财经大事一览</h1>
        <table border="0"  cellpadding="0" cellspacing="0">
        	<tr>
            	<th>时间</th>
                <th>国家（地区）</th>
                <th class="da_data da_data3">地点</th>
            	<th>重要性</th>
                <th class="da_data da_data2">事件</th>
            </tr>
            <% if(ImportThings.length>0) {%>
                  <% for (var i = 0; i < ImportThings.length; i++) { %>
                  <tr>
                      <td><%=ImportThings[i]["PredictTime"] %></td>
                      <td><%=ImportThings[i]["State"]%></td>
                      <td>
                          <span title="--">--</span>
                      </td>
                      <td class="da_import">
                           <span class="da_mid">
                              <% if(ImportThings[i]["Nature"]=="高"){ %>
                              <img src="images/ix_degree2.jpg"  width="28" height="22">
                              <%} else if(ImportThings[i]["Nature"]=="中") { %>
                              <img src="images/ix_degree.jpg"  width="28" height="22">
                              <%} else if(ImportThings[i]["Nature"]=="低") { %>
                              <img src="images/ix_degree3.jpg"  width="28" height="22">
                              <% } %></span>
                      </td>
                      <td><%=ImportThings[i]["NFI"]%></td>
                  </tr>
                  <% } %>
            <% }else{%>
                 <tr><td colspan="5">今天无重要大事预告公布！</td> </tr>
            <% } %>
        </table>
    </div>
	<div class="da_form">
    	<h1><span class="curDate"><%=currentDate%></span>  假期预告一览</h1>
        <table border="0"  cellpadding="0" cellspacing="0">
        	<tr>
            	<th>时间</th>
                <th>国家（地区）</th>
                <th class="da_data da_data3">地点</th>
            	<th>重要性</th>
                <th class="da_data da_data2">事件</th>
            </tr>
             <% if(HolidayNotices.length>0) {%>
                  <% for (var i = 0; i < HolidayNotices.length; i++) { %>
                  <tr>
                      <td><%=HolidayNotices[i]["PredictTime"] %></td>
                      <td><%=HolidayNotices[i]["State"]%></td>
                      <td>
                          <span title="--">--</span>
                      </td>
                      <td class="da_import">
                          <span class="da_mid" >
                              <% if(HolidayNotices[i]["Nature"]=="高"){ %>
                              <img src="images/ix_degree2.jpg"  width="28" height="22">
                              <%} else if(HolidayNotices[i]["Nature"]=="中") { %>
                              <img src="images/ix_degree.jpg"  width="28" height="22">
                              <%} else if(HolidayNotices[i]["Nature"]=="低") { %>
                              <img src="images/ix_degree3.jpg"  width="28" height="22">
                              <% } %></span>
                      </td>
                      <td><%=HolidayNotices[i]["NFI"]%></td>
                  </tr>
                  <% } %>
            <% }else{%>
                 <tr><td colspan="5">今天无重要节日预告公布！</td> </tr>
            <% } %>
        </table>
    </div>
</div>
